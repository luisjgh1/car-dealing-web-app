.search-box-container
  .search-box
    ul#myTab.nav.nav-tabs(role='tablist')
      li.nav-item.d-flex.align-items-center.pl-2
        img.d-block.img-item-nav(src=searchForm.tabOneIcon)
        a#home-tab.nav-link.active(data-toggle='tab', href='#home', role='tab', aria-controls='home', aria-selected='true')=searchForm.tabOneText
      li.nav-item.d-flex.align-items-center.pl-2.unselected
        img.d-block.img-item-nav(src=searchForm.tabTwoIcon)
        a#profile-tab.nav-link(data-toggle='tab', href='#profile', role='tab', aria-controls='profile', aria-selected='false')=searchForm.tabTwoText
    #myTabContent.tab-content
      #home.tab-pane.fade.show.active(role='tabpanel', aria-labelledby='home-tab')
        #search-form
          search-form

      #profile.tab-pane.fade(role='tabpanel', aria-labelledby='profile-tab')
        iframe(src="/setster/", style="height: 300px; width: 100%; border: none;")
